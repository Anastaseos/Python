import numpy as np

matrix_1 = np.array([[1,2], [3,4]])
matrix_2 = np.array([[7,8], [9,10]])

# new_matrix = np.dot(matrix_1, matrix_2)
# print(new_matrix)

matrix_sum = matrix_1 + matrix_2
print(matrix_sum)

matrix_diff = matrix_1 - matrix_2
print(matrix_diff)

matrix_mult = matrix_1 * matrix_2
print(matrix_mult)

matrix_div = matrix_1 / matrix_2
print(matrix_div)

matrix_determinant = np.linalg.det(matrix_1)
print(matrix_determinant)

matrix_inverse = np.linalg.inv(matrix_1)
print(matrix_inverse)

